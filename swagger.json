{
  "openapi": "3.0.1",
  "info": {
    "title": "Fast-Food-Fast",
    "description": "Fast-Food-Fast is a food delivery service app for a restaurant [Fast-Food-Fast](https://fejiroofficial.github.io/Fast-Food-Fast/UI/index.html)",
    "version": "0.1",
    "contact": {
      "email": "gpsparks@rocketmail.com"
    }
  },
  "servers": [
    {
      "url": "https://food-fast-app.herokuapp.com"
    }
  ],
  "tags": [
    {
      "name": "Users",
      "description": "Creating an account as a user",
      "externalDocs": {
        "description": "User registration and login",
        "url": "https://food-fast-app.herokuapp.com/"
      }
    },
    {
      "name": "Orders",
      "description": "Everything about making an order",
      "externalDocs": {
        "description": "Find out more about placing an order",
        "url": "https://food-fast-app.herokuapp.com/"
      }
    },
    {
      "name": "Menu",
      "description": "Details of the food menu",
      "externalDocs": {
        "description": "Find out more about the food menu",
        "url": "https://food-fast-app.herokuapp.com/"
      }
    },
    {
      "name": "Cart",
      "description": "Details about the cart",
      "externalDocs": {
        "description": "Find out more about the food menu",
        "url": "https://food-fast-app.herokuapp.com/"
      }
    }
  ],
  "paths": {
    "/api/v1/auth/login": {
      "post": {
        "tags": [
          "Users"
        ],
        "summary": "Login a user",
        "description": "Route to log a user in",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "email": {
                    "type": "string"
                  },
                  "password": {
                    "type": "string"
                  }
                }
              },
              "examples": {
                "0": {
                  "value": "{\n    \"email\": \"house@gmail.com\",\n    \"password\": \"jig\"\n}"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successfully logged user in",
            "content": {
              "text/html; charset=utf-8": {
                "schema": {
                  "type": "string"
                },
                "examples": {}
              }
            }
          }
        }
      }
    },
    "/api/v1/auth/signup": {
      "post": {
        "tags": [
          "Users"
        ],
        "summary": "creates a new user",
        "description": "Route to register a new user",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "firstname": {
                    "type": "string"
                  },
                  "lastname": {
                    "type": "string"
                  },
                  "email": {
                    "type": "string"
                  },
                  "telephone": {
                    "type": "integer"
                  },
                  "password": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Endpoint successfully registers the new user",
            "content": {
              "text/html; charset=utf-8": {
                "schema": {
                  "type": "string"
                },
                "examples": {}
              }
            }
          }
        }
      }
    },
    "/api/v1/orders": {
      "get": {
        "tags": [
          "Orders"
        ],
        "summary": "Get all orders",
        "description": "Get all orders",
        "responses": {
          "200": {
            "description": "Successfully returns all orders in the database",
            "content": {
              "application/json; charset=utf-8": {
                "schema": {
                  "type": "string"
                },
                "examples": []
              }
            }
          }
        }
      },
      "post": {
        "tags": [
          "Orders"
        ],
        "summary": "make an order for food",
        "description": "Route to post an order for food items",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "deliveryAddress": {
                    "type": "string"
                  },
                  "telephone": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Successfully post an order",
            "content": {
              "text/html; charset=utf-8": {
                "schema": {
                  "type": "string"
                },
                "examples": {}
              }
            }
          }
        }
      }
    },
    "/api/v1/users/{userId}/orders": {
      "get": {
        "tags": [
          "Orders"
        ],
        "summary": "Get order history",
        "description": "Get the order history for a particular user.",
        "parameters": {
          "name": "userId",
          "in": "path",
          "description": "The Id of the user",
          "type": "integer"
        },
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "array",
                "properties": {
                  "answer": {
                    "type": "string"
                  },
                  "userid": {
                    "type": "integer"
                  },
                  "username": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successfully gets history of ordered food items by a user",
            "content": {
              "text/html; charset=utf-8": {
                "schema": {
                  "type": "string"
                },
                "examples": []
              }
            }
          }
        }
      }
    },
    "/api/v1/orders/{orderId}": {
      "get": {
        "tags": [
          "Orders"
        ],
        "summary": "Get an order",
        "description": "Route to fetch a specific order",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "orderId": {
                    "type": "integer"
                  }
                }
              },
              "examples": {
                "0": {
                  "value": "{\n    \"orderId\": 3\n}"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "successfully gets a specific order",
            "content": {
              "text/html; charset=utf-8": {
                "schema": {
                  "type": "string"
                },
                "examples": {}
              }
            }
          }
        }
      },
      "put": {
        "tags": [
          "Orders"
        ],
        "summary": "update the Status of an order. Status could either be New , Processing , Cancelled or Complete ",
        "description": "Route to update the status of an order",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "userId": {
                    "type": "integer"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "successfully updates the order status",
            "content": {
              "text/html; charset=utf-8": {
                "schema": {
                  "type": "string"
                },
                "examples": {}
              }
            }
          }
        }
      }
    },
    "/api/v1/menu": {
      "get": {
        "tags": [
          "Menu"
        ],
        "summary": "Get all food items on the menu",
        "description": "Route to get food menu",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "question": {
                    "type": "string"
                  },
                  "userId": {
                    "type": "integer"
                  },
                  "username": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successfully gets food menu if any is found",
            "content": {
              "text/html; charset=utf-8": {
                "schema": {
                  "type": "string"
                },
                "examples": {}
              }
            }
          }
        }
      },
      "post": {
        "tags": [
          "Menu"
        ],
        "summary": "add food to menu",
        "description": "Route for an admin to post food into the menu",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "itemName": {
                    "type": "string"
                  },
                  "price": {
                    "type": "integer"
                  },
                  "foodUrl": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Successfully adds food to menu",
            "content": {
              "application/json; charset=utf-8": {
                "schema": {
                  "type": "string"
                },
                "examples": {}
              }
            }
          }
        }
      }
    },
    "/api/v1/cart": {
      "post": {
        "tags": [
          "Cart"
        ],
        "summary": "add food items to cart",
        "description": "Route to post a food menu into a cart",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "foodId": {
                    "type": "string"
                  },
                  "quantity": {
                    "type": "integer"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Successfully posts food menu into the cart",
            "content": {
              "text/html; charset=utf-8": {
                "schema": {
                  "type": "string"
                },
                "examples": {}
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "oauth2": {
        "type": "oauth2",
        "flows": {
          "implicit": {
            "authorizationUrl": "http://yourauthurl.com",
            "scopes": {
              "scope_name": "Enter your scopes here"
            }
          }
        }
      }
    }
  }
}